enum Instrument {
    Sax = 0;
    Trumpet = 1;
    Clarinet = 2;
}

/* test */
[opcode("JAZZ")]
readonly struct Musician {
    /* a name */
    string name;
	/* an instrument */
    Instrument plays;
}

message Song {
    1 -> string title;
    2 -> uint16 year;
    3 -> Musician[] performers;
}

struct Library {
    map[guid, Song] songs;
}

union Album {
    1 -> struct StudioAlbum {
        Song[] tracks;
    }
    2 -> message LiveAlbum {
        1 -> Song[] tracks;
        2 -> string venueName;
        3 -> date concertDate;
    }
}