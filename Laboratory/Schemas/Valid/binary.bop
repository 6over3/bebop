enum Status : byte {
    Okay = 1;
    Doable = 2;
}

union Power {
    1 -> readonly struct Point {
        int32 x;
        int32 y;
        string name;
        Status status;
        int32[] numbers;
        map[string, string] aMap;
    };
    2 -> message Coordinate {
        1 -> Point point;
        2 -> guid id;
        3 -> map[string, map[string, string[][]]] nested;
    };
}