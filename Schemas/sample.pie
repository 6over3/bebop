package the_more_you_know;

enum VideoCodec {
  H264 = 0;
  H265 = 1;
}

readonly struct VideoData {
  float timestamps;
  uint   width;
  uint   height;
  byte[] fragment;
}

message MyMessage {
  uint cats = 1;
  uint dogs = 2;
}

message MediaMessage {
  VideoCodec[] codecs = 1;
  VideoCodec otherCodec = 2;
  VideoData data = 3;
  MyMessage mine = 4 [deprecated("why")];
}
